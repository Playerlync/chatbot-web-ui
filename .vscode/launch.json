    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "vuejs: chrome",
        "url": "http://localhost:8081",
        "webRoot": "${workspaceFolder}",
        "breakOnLoad": true,
        "pathMapping": {
          "/_karma_webpack_": "${workspaceFolder}"
        },
        // "sourceMapPathOverrides": {
        //   "webpack:/*": "${webRoot}/*",
        //   "/./*": "${webRoot}/*",
        //   "/src/*": "${webRoot}/*",
        //   "/*": "*",
        //   "/./~/*": "${webRoot}/node_modules/*"
        // },
        "preLaunchTask": "vuejs: start",
        // "skipFiles": [
        //   "<node_internals>/**",
        //   "${workspaceFolder}/node_modules/**/*.js"
        // ],
        "skipFiles": [
          "<node_internals>/**/*.js"
        ]
      }
    ]
  }